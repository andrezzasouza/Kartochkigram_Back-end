{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard-with-typescript", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"

  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"]
    }
  ],
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "comma-dangle": "off"
  }
}
